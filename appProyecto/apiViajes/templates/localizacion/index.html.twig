{% extends 'base.html.twig' %}

{% block title %}Localizacion index{% endblock %}

{% block body %}
    <h1>Localizacion index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Codigo</th>
                <th>Nombre</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for localizacion in localizacions %}
            <tr>
                <td>{{ localizacion.id }}</td>
                <td>{{ localizacion.codigo }}</td>
                <td>{{ localizacion.nombre }}</td>
                <td>
                    <a href="{{ path('app_localizacion_show', {'id': localizacion.id}) }}">show</a>
                    <a href="{{ path('app_localizacion_edit', {'id': localizacion.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="4">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_localizacion_new') }}">Create new</a>
{% endblock %}
